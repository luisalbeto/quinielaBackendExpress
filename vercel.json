{
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/api/users/(.*)",
            "dest": "/api/users/$1"
          },
          {
            "src": "/api/(.*)",
            "dest": "/api/$1"
          },
          {
            "src": "/(.*)",
            "dest": "/"
          }
   
    ]
  }